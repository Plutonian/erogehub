<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.goexp.galgame.gui.view.brand.TitlePartController">
    <FlowPane prefWrapLength="800.0" rowValignment="BOTTOM" AnchorPane.bottomAnchor="0.0"
              AnchorPane.topAnchor="0.0">
        <MenuButton fx:id="menuComp" mnemonicParsing="false" styleClass="copyablelabel"
                    stylesheets="@/view/view.css" text="########">
            <font>
                <Font size="18.0"/>
            </font>
        </MenuButton>
        <Text fx:id="txtComp" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
    </FlowPane>
    <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
        <Button fx:id="btnBlock" text="Block"/>
        <ChoiceBox fx:id="choiceBrandState" visible="false"/>
        <!--        <HBox fx:id="boxWebsite" spacing="10.0"/>-->
    </HBox>
</AnchorPane>
