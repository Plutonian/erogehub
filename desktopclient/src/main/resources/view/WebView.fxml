<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.goexp.galgame.gui.view.pagesource.WebViewController">
    <stylesheets>
        <URL value="@view.css" />
    </stylesheets>

    <center>
        <WebView fx:id="webView">

        </WebView>
    </center>
    <top>
        <HBox spacing="10" BorderPane.alignment="CENTER">
            <TextField fx:id="locationField" editable="false" HBox.hgrow="ALWAYS" />
            <Button fx:id="goButton" onAction="#goButton_OnAction" text="Set As Brand Site" />
        </HBox>
    </top>
    <bottom>
        <Label fx:id="lbUrl" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
