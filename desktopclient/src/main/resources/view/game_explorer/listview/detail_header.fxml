<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<BorderPane xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.goexp.galgame.gui.view.game.listview.SimpleListViewController$CellController$HeaderController">
    <stylesheets>
        <URL value="@../../view.css" />
    </stylesheets>
    <center>
        <BorderPane>
            <center>
            <BorderPane>
               <BorderPane.margin>
                  <Insets left="10.0" />
               </BorderPane.margin>
               <center>
                      <VBox>
                          <children>
                              <HBox>
                                  <children>
                                      <Text fx:id="txtName" strokeType="OUTSIDE" strokeWidth="0.0" text="OH!!マイクロマン">
                                          <font>
                                              <Font name="System Bold" size="24.0" />
                                          </font>
                                      </Text>
                                      <Text fx:id="txtSubName" strokeType="OUTSIDE" strokeWidth="0.0" text="～小さくなって女の子の色んなトコロに入っちゃお～" />
                                  </children>
                              </HBox>
                              <fx:include fx:id="brandJump" source="../../jump/brandjump.fxml" />
                              <HBox fx:id="boxStar" />
      
                              <HBox fx:id="boxTag" spacing="5.0" />
                              <GridPane>
                                  <columnConstraints>
                                      <ColumnConstraints hgrow="NEVER" maxWidth="63.0" minWidth="10.0" prefWidth="19.0" />
                                      <ColumnConstraints hgrow="ALWAYS" maxWidth="600.0" minWidth="10.0" prefWidth="449.0" />
                                  </columnConstraints>
                                  <rowConstraints>
                                      <RowConstraints vgrow="SOMETIMES" />
                                      <RowConstraints fillHeight="false" vgrow="SOMETIMES" />
                                  </rowConstraints>
                                  <children>
                                      <Label text="原" textFill="#6e6d6d" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                                      </Label>
                                      <FlowPane fx:id="flowPainter" rowValignment="TOP" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.vgrow="SOMETIMES" />
                                      <Label text="シ" textFill="#6e6d6d">
                                          <font>
                                              <Font name="System Bold" size="12.0" />
                                          </font>
                                      </Label>
                                      <Label fx:id="txtWriter" text="XX" wrapText="true" GridPane.columnIndex="1" />
                                  </children>
                                  <opaqueInsets>
                                      <Insets />
                                  </opaqueInsets>
                              </GridPane>
      
                              <!--<fx:include fx:id="starChange" source="/view/game_explorer/detail/game_star_choicebar.fxml" />-->
      
                          </children>
                      </VBox>
               </center>
               <bottom>
                  <VBox alignment="BOTTOM_CENTER" style="-fx-background-color: rgba(200,200,100,0.4);" BorderPane.alignment="CENTER">
                     <children>
                            <Hyperlink fx:id="linkView" text="View" />
                     </children>
                  </VBox>
               </bottom>
               <right>
                        <fx:include fx:id="changeState" source="../detail/game_statechange.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" StackPane.alignment="BOTTOM_LEFT" />
               </right>
            </BorderPane>
            </center>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <left>
                <AnchorPane BorderPane.alignment="CENTER">
                    <children>
                        <ImageView fx:id="imageImg" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <image>
                                <Image url="@../../../../../../../../gal_data/img/game/3369/game_s.jpg" />
                            </image>
                        </ImageView>
                    </children>
                    <BorderPane.margin>
                        <Insets />
                    </BorderPane.margin>
                </AnchorPane>
            </left>
        </BorderPane>
    </center>
    <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
    </padding>
   <left>
        <StackPane BorderPane.alignment="CENTER">
            <children>
                <fx:include fx:id="dateview" source="../detail/date.fxml" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" BorderPane.alignment="TOP_CENTER" StackPane.alignment="TOP_LEFT" />
                <!--<fx:include fx:id="webjump" source="../../jump/websitejump.fxml" StackPane.alignment="BOTTOM_CENTER" />-->
            </children>
         <padding>
            <Insets left="3.0" right="3.0" />
         </padding>
        </StackPane>
   </left>
</BorderPane>
