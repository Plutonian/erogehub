<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="com.goexp.galgame.gui.view.game.listview.ImgListViewController$CellController">
    <stylesheets>
        <URL value="@../../../view.css"/>
    </stylesheets>
    <children>
        <StackPane>
            <children>
                <ImageView fx:id="imageImg" pickOnBounds="true" preserveRatio="true"/>
                <AnchorPane maxHeight="-Infinity" prefHeight="20.0" style="-fx-background-color: #00000025;"
                            StackPane.alignment="TOP_CENTER">
                    <children>
                        <Label fx:id="lbDate" text="2001-5-6" textFill="WHITE" AnchorPane.rightAnchor="10.0"
                               AnchorPane.topAnchor="0.0">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                    <padding>
                        <Insets bottom="3.0" top="3.0"/>
                    </padding>
                </AnchorPane>
                <HBox fx:id="boxStar" maxHeight="-Infinity" prefHeight="32.0" StackPane.alignment="BOTTOM_RIGHT"/>
            </children>
        </StackPane>
        <VBox>
            <children>
                <Text fx:id="txtName" strokeType="OUTSIDE" strokeWidth="0.0" text="緋奈沢智花の絶対女王政" wrappingWidth="300.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Text>
                <Label fx:id="lbBrand" text="Brand"/>
                <FlowPane fx:id="flowTag" prefWrapLength="200.0"/>
            </children>
        </VBox>
        <AnchorPane>
            <children>
                <HBox spacing="5.0">
                    <children>
                        <fx:include fx:id="changeState" source="../../detail/game_statechange.fxml"/>
                    </children>
                </HBox>
                <Hyperlink fx:id="linkView" text="View" layoutX="256.0" layoutY="2.0" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.rightAnchor="10.0"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
