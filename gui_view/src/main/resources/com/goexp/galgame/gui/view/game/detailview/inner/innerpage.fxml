<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.net.URL?>
<TabPane fx:id="contentTabPane" side="RIGHT" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.goexp.galgame.gui.view.game.detailview.inner.InnerPageController">
    <stylesheets>
        <URL value="@/view/view.css"/>
    </stylesheets>
    <Tab text="Intro">
        <fx:include fx:id="header" source="header.fxml"/>
    </Tab>
    <Tab fx:id="tabPerson" text="キャラクター">
        <ListView fx:id="personListView" styleClass="copyablelabel"/>
    </Tab>
    <Tab fx:id="tabSimple" text="サンプル画像">
        <fx:include fx:id="simpleImg" source="simple_img.fxml"/>
    </Tab>
</TabPane>
